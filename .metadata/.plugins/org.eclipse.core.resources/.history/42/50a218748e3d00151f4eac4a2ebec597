		String title = new String("");
		
		iter = 0;
		section = 1;
		while (true) {
			iter++;
			section *= num;
			if (n < section) {
				for (int i = 0; i < iter; i++) {
					
				}
			} else {
				n -= section;
			}
		}
	}
}
