package problemset;

import java.util.Stack;

public class IsValidParentheses {
	public boolean solution(String s)
	{
		Stack<Character> paren = new Stack<>();
		
		for (int i = 0; i < s.length(); i++) {
			char c = s.charAt(i);
			if ((c == '(') || (c == '[') || (c == '{')) {
				paren.push(c);
			}
		}
	}
}
