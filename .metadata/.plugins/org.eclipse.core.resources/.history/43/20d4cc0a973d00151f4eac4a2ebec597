package problemset;

public class PlusOne {
	public int[] solution(int[] digits)
	{
		int len = digits.length;
		
		if (digits[len-1] != 9) {
			digits[len-1] += 1;
			return digits;
		} else {
			int all9 = 1;
			int[] sum = new int[len + 1];
			for (int i = len - 2; i >= 0; i--) {
				if (digits[i] != 9) {
					all9 = 0;
					break;
				}
			}
			if (all9 == 1) {
				for (int i = 1; i < len + 1; i++) sum[i] = 0;
				sum[0] = 1;
			} else {
				
			}
		}
	}
}
