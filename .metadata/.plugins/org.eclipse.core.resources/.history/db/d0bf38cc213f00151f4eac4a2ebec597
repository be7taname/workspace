package problemset;

public class ClimbStairs {
	public int solutionRecur(int n)
	{
		if (n == 1) return 1;
		else if (n == 2) return 2;
		else {
			return solutionRecur(n-1) + 2*solutionRecur(n-2);
		}
	}

	public int solutionIter(int n)
	{
		if (n == 1) return 1;
		else if (n == 2) return 2;
		else {
			int last = 1, curr = 2, next;
			for (int i = 3; i <= n; i++) {
				
			}
		}
	}
}
